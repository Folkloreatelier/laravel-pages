{
    "name": "folklore/pages",
    "description": "Laravel 4 and 5 package to deal with Pages and Blocks in a content management system (CMS)",
    "keywords": ["laravel","pages","blocks","eloquent","content","cms"],
    "homepage": "http://github.com/Folkloreatelier/laravel-pages",
    "license": "MIT",
    "authors": [
        {
            "name": "Folklore",
            "email": "info@atelierfolklore.ca",
            "homepage": "http://atelierfolklore.ca"
        },
        {
            "name": "David Mongeau-Petitpas",
            "email": "dmp@atelierfolklore.ca",
            "homepage": "http://mongo.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "folklore/eloquent-picturable": "~0.2",
        "folklore/eloquent-localizable": "~0.2",
        "cviebrock/eloquent-sluggable": "dev-master",
        "cviebrock/eloquent-taggable": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Folklore\\Pages\\": "src/"
        }
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
